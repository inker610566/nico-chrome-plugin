{
    "name": "nico-chrome-plugin",
    "description": "redirect to service from http://www.nicozon.net",
    "version": "1.0",
    "minimum_chrome_version": "16.0.884",
    "permissions": ["downloads", "<all_urls>"],
    "content_security_policy": "script-src 'self' https://ext.nicovideo.jp; object-src 'self'",
    "browser_action": {"default_popup": "popup.html"},
    "manifest_version": 2
}
